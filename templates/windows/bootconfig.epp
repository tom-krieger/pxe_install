{
  "version": <%= $osversion %>,
  <%- if ! empty($iso) { -%>
  "iso": "<%= $iso %>",
  <%- } -%>
  "boot_architecture": "<%= $boot_architecture %>",
  "hostname": "<%= $hostname %>",
  "ipaddress": "<%= $fixedaddress %>",
  "subnet": "<%= $subnet %>",
  "gateway": "<%= $gateway %>",
  "dns": [ <%= $dns_servers %> ]
}
