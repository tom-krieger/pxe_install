        menu default
        com32 linux.c32
        APPEND <%= $mirror_host %><%= $mirror_uri %>/wimboot initrdfile=<%= $mirror_host %><%= $mirror_uri %>/boot/bootmgr,<%= $mirror_host %><%= $mirror_uri %>/boot/bcd,<%= $mirror_host %><%= $mirror_uri %>/boot/boot.sdi,<%= $mirror_host %><%= $mirror_uri %>/sources/boot.wim
