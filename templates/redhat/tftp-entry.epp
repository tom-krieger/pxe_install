prompt 0
timeout 1
display <%= $prefix %>/boot.msg
default ks

label ks
        kernel <%= $prefix %>/vmlinuz
        append <%= $ks %>=<%= $ksurl %> initrd=<%= $prefix %>/initrd.img devfs=nomount ksdevice=<%= $ksdevice %> puppetenv=<%= $puppetenv %> puppetrole=<%= $puppetrole %> dc=<%= $datacenter %> orgid=<%= $orgid %> actkey=<%= $actkey %>
